{
  "name": "goosefactory",
  "version": "1.1.3",
  "description": "Factory for creating the redux-saga analogy to a redux duck: a goose",
  "main": "lib/index.js",
  "scripts": {
    "compile": "rimraf lib/* && babel src -d lib",
    "prepublish": "npm run checkall && npm run compile",
    "test": "NODE_PATH=./ mocha --compilers js:babel-core/register --require babel-polyfill \"test/**/*@(.js)\"",
    "eslint": "eslint ./*.js src/ test/",
    "checkall": "npm run eslint & npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/espen42/goosefactory.git"
  },
  "author": "Espen Norderud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/espen42/goosefactory/issues"
  },
  "homepage": "https://github.com/espen42/goosefactory#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "redux-saga": "^0.14.3"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "3.5.0",
    "eslint": "3.14.0",
    "eslint-plugin-react": "^6.10.0",
    "jsdom": "^9.11.0",
    "mocha": "3.2.0",
    "rimraf": "^2.6.1"
  }
}
